      <div class="panel panel-default">

        <div class="panel-body">
    <% @posts.each do |post| %>
          <div class="page-header">
            <h3><%= link_to post.title, post %><small> <span class="label label-danger">New</span>(<%= post.created_at%>)</small></h3>
          </div>
          <%= post.content %>
      <% end %>
        </div>
      </div>
 <ul class="pager">
    <li class="previous"><a href="#">&larr; Older</a></li>
    <li class="next"><a href="#">Newer &rarr;</a></li>
  </ul>

